{
    "name": "spiral/data-grid-bridge",
    "type": "library",
    "description": "Data Grid specification builder adapter for Spiral Framework",
    "license": "MIT",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/data-grid-bridge"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "spiral/attributes": "^2.10 || ^3.0",
        "spiral/boot": "^3.0",
        "spiral/data-grid": "^3.0",
        "spiral/http": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "spiral/hmvc": "^3.0",
        "vimeo/psalm": "^4.27"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\DataGrid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\DataGrid\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-3.0": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
